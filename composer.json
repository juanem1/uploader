{
    "name": "ibox/uploader",
    "description": "File uploader for laravel framework.",
    "keywords": ["laravel", "File uploader", "S3"],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "support": {
        "issues": "https://github.com/juanem1/uploader/issues",
        "source": "https://github.com/juanem1/uploader"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Emilio Turk",
            "email": "juanemilioturk@gmail.com",
            "homepage": "http://ideasbox.com.ar"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ibox\\Uploader\\": "src/"
        }
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (~1.0).",
        "league/flysystem-rackspace": "Required to use the Flysystem Rackspace driver (~1.0)."
    },
    "minimum-stability": "dev"
}
